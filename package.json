{
  "name": "applied-fp-ts",
  "homepage": "https://github.com/doubleloop-io/applied-fp-workshop-ts",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "peerDependencies": {
    "fp-ts": "2.13.1"
  },
  "dependencies": {
    "ts-pattern": "^4.0.6"
  },
  "devDependencies": {
    "@types/jest": "26.0.20",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "cspell": "6.18.0",
    "eslint": "7.32.0",
    "fp-ts": "2.13.1",
    "jest": "26.6.3",
    "prettier": "2.8.1",
    "prettier-plugin-jsdoc": "0.4.2",
    "ts-jest": "26.5.6",
    "typescript": "4.9.4"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc -w -p tsconfig.build.json",
    "prepublish": "yarn run build",
    "pretty": "yarn run prettier --write .",
    "lint": "yarn run eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "test": "yarn run jest",
    "test:watch": "yarn run jest --watch",
    "spell": "yarn cspell '**/*.*'"
  }
}
