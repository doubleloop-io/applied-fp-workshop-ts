{
  "name": "applied-fp-ts",
  "homepage": "https://github.com/doubleloop-io/applied-fp-workshop-ts",
  "version": "1.0.0",
  "license": "MIT",
  "author": {
    "name": "Matteo Baglini",
    "email": "matteo.baglini@gmail.com"
  },
  "devDependencies": {
    "@types/node": "24.8.1",
    "@types/jest": "30.0.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "cspell": "9.2.1",
    "eslint": "8.57.1",
    "fastify": "5.6.1",
    "fp-ts": "2.16.11",
    "jest": "30.2.0",
    "prettier": "3.6.2",
    "prettier-plugin-jsdoc": "1.3.3",
    "ts-jest": "29.4.5",
    "ts-node": "10.9.2",
    "ts-pattern": "5.9.0",
    "typescript": "5.9.3"
  },
  "scripts": {
    "clean": "rm -rf dist node_modules",
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0 --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "spell": "cspell '**/*.*'",
    "eval": "ts-node",
    "app": "yarn build && node dist/app/index.js",
    "web": "yarn build && node dist/webapp/index.js"
  }
}
